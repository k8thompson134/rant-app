{
  "name": "ranttrack-mobile",
  "version": "0.1.0",
  "description": "Privacy-first symptom tracker for chronically ill people",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "node scripts/build-variant.js dev && expo run:android",
    "ios": "node scripts/build-variant.js dev && expo run:ios",
    "android:prod": "node scripts/build-variant.js prod && expo run:android && node scripts/build-variant.js dev",
    "ios:prod": "node scripts/build-variant.js prod && expo run:ios && node scripts/build-variant.js dev",
    "web": "expo start --web",
    "tsc": "tsc --noEmit",
    "postinstall": "npx jetify",
    "prepare": "cd .. && husky || true"
  },
  "dependencies": {
    "@expo-google-fonts/dm-sans": "^0.4.2",
    "@expo-google-fonts/dm-serif-display": "^0.4.2",
    "@expo/vector-icons": "^15.0.3",
    "@jamsch/expo-speech-recognition": "~0.2.15",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "~7.9.0",
    "@react-navigation/native": "~7.1.26",
    "@react-navigation/native-stack": "~7.9.0",
    "@react-navigation/stack": "~7.6.13",
    "chrono-node": "^2.9.0",
    "drizzle-kit": "~0.31.8",
    "drizzle-orm": "~0.45.1",
    "expo": "~54.0.30",
    "expo-blur": "^15.0.8",
    "expo-clipboard": "^8.0.8",
    "expo-constants": "^18.0.12",
    "expo-device": "^8.0.10",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-sharing": "~14.0.8",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.15",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.19.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "expo-dev-client": "~6.0.20",
    "husky": "^9.1.7",
    "jetifier": "^2.0.0",
    "lint-staged": "^16.2.7",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "bash -c 'npm run tsc'"
    ]
  },
  "private": true
}
